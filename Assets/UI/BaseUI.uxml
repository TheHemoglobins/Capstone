            <ui:Label display-tooltip-when-elided="true" name="Title" text="VR Museum" enable-rich-text="false" style="font-size: 32px; -unity-font: url(&apos;project://database/Library/unity%20default%20resources?fileID=10102&amp;guid=0000000000000000e000000000000000&amp;type=0#Arial&apos;); color: rgb(34, 34, 34); width: auto; height: auto; white-space: normal; -unity-text-align: upper-center; margin-left: 0; align-items: flex-start; justify-content: flex-start; flex-direction: row; flex-wrap: nowrap; flex-grow: 0; flex-shrink: 1;" />
            <ui:Label text="VR Museum is a project dedicated to the learning and usage of Unity in collaboration with VR. It allows users to upload images of impactful moments in their life in order to relieve them" display-tooltip-when-elided="true" name="ProjectDescription" style="width: auto; height: auto; font-size: 16px; color: rgb(0, 0, 0); margin-left: 0; -unity-text-align: middle-center; white-space: normal; padding-bottom: 5px; -unity-font: url(&apos;project://database/Library/unity%20default%20resources?fileID=10102&amp;guid=0000000000000000e000000000000000&amp;type=0#Arial&apos;); justify-content: center; margin-top: 0;" />
